            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::avs.dev.zixiao.wang"
            ]
        },
        {
            "Action": [
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Effect": "Allow",
